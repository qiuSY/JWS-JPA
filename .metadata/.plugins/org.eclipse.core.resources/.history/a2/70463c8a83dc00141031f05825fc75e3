package edu.neu.cs5200.Site.models;

import javax.persistence.*;

@Entity
public class Equipment {
	@Id
	
	private Integer id;
	private String name;
	private String brand;
	private String description;
	private Float price;
	
	@ManyToOne(fetch=FetchType.LAZY)
	@JoinColumn(name="towerId")
	
	private Tower tower;
	

}
